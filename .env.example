# Discord Bot 設定
BOT_TOKEN=YOUR_BOT_TOKEN_HERE
APPLICATION_ID=YOUR_APPLICATION_ID_HERE
HOST_USER_ID=YOUR_DISCORD_USER_ID_HERE

# WAR計算API 設定
API_URL=YOUR_API_URL_HERE
BASIC_ID=YOUR_BASIC_AUTH_ID_HERE
BASIC_PASS=YOUR_BASIC_AUTH_PASS_HERE

# Node環境 (development または production)
NODE_ENV=development

# ログレベル (error, warn, info, http, verbose, debug, silly)
# 'debug' に設定すると、APIペイロードやユーザー入力を含む詳細な出力が表示されます。
LOG_LEVEL=info